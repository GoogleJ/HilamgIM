<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="45dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginStart="-20dp"
    android:layout_height="45dp">


    <io.rong.imkit.widget.AsyncImageView
        android:id="@+id/ivMemberHead"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_gravity="center"
        app:RCDefDrawable="@drawable/rc_ic_def_msg_portrait"
        app:RCShape="circle" />

    <ImageView
        android:id="@+id/ivOwner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_marginTop="0.6dp"
        android:layout_marginEnd="0.6dp"
        android:rotation="6"
        android:src="@drawable/ic_social_owner"
        android:visibility="gone" />

    <TextView
        android:id="@+id/tvNumLeft"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_gravity="center"
        android:background="@drawable/shape_e5ebf2_circle"
        android:gravity="center"
        android:textColor="@color/colorTheme"
        android:textSize="12sp"
        android:visibility="gone" />

</FrameLayout>